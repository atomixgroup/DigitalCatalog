<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <attr name="fontName" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomButton">
        <attr name="btnFontName" format="string" />
    </declare-styleable>
    <declare-styleable name="RoundRectCornerImageView">
        <attr name="round" format="float" />
    </declare-styleable>
    <declare-styleable name="RatingBarAttributes">
        <attr name="numStars" format="integer" />
        <attr name="rating" format="float" />
        <attr name="starPadding" format="integer" />
        <attr name="drawableEmpty" format="reference" />
        <attr name="drawableFilled" format="reference" />
        <attr name="touchable" format="boolean" />
        <attr name="clearRatingEnabled" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomLoginInput">
        <attr name="iconDr" format="reference" />
        <attr name="hintText" format="string" />
        <attr name="edtType" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomInput">
        <attr name="inputLabelText" format="string" />
        <attr name="inputEdtType" format="string" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
</resources>